<h2 class="h4 font-weight-bolder mt-5 mb-3">Teacher Form</h2>

<div class="tab-content pt-3" id="myTabContent">
  <div
    class="tab-pane fade show active"
    id="game"
    role="tabpanel"
    aria-labelledby="game-tab"
  >
    <form method="post" action="/admin/user-game">
      <div class="form-group">
        <label for="forUsername">Username</label>
        <input
          id="formName"
          type="text"
          name="username"
          class="form-control col-md-4"
        />
      </div>
      </div>
      <div class="form-group">
        <label for="forPassword">Password</label>
        <input
          id="forPassword"
          type="password"
          name="password"
          class="form-control col-md-4"
        />
      </div>
      <button class="btn btn-success" type="submit">Create</button>
    </form>
  </div>
  <div
    class="tab-pane fade"
    id="biodata"
    role="tabpanel"
    aria-labelledby="history-tab"
  >
    
  </div>
  </div>
</div>
<center>
<tbody>
  <% for( let i = 0; i < userbiodata.length; i++ ) { %>
  <br><tr>
    <td><%= userbiodata[i].fullname %></td>
  </tr>
  <% } %>
</tbody>